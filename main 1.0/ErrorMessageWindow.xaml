<Window x:Class="main_1._0.ErrorMessageWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:self="clr-namespace:main_1._0"
        mc:Ignorable="d"
        Title="ErrorMessageWindow" Height="200" Width="600" MinHeight="200" MaxHeight="200" MinWidth="600" MaxWidth="600">

    <Window.CommandBindings>
        <CommandBinding Command="self:KGPcommands.EMWconfirm" CanExecute="EMWconfirm_CanExecute" Executed="EMWconfirm_Executed"/>
    </Window.CommandBindings>
    
    <Grid Background="#FFCDCDCD">
        <TextBlock Name="ErrorShowingText"  Margin="39,10,59.6,66.4" FontSize="20" TextAlignment="Center" TextWrapping="Wrap"></TextBlock>
        <Button Margin="214,104,210,27" FontSize="18" Command="self:KGPcommands.EMWconfirm">OK</Button>
    </Grid>
</Window>
