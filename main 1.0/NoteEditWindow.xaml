<Window x:Class="main_1._0.NoteEditWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:self="clr-namespace:main_1._0"
        mc:Ignorable="d"
        Title="NoteEditWindow" Height="200" Width="300">

    <Window.CommandBindings>
        <CommandBinding Command="self:KGPcommands.ApplyChangesNEW" CanExecute="AlwaysTrueForCanExecute" Executed="ApplyChanges_Executed" />
        <CommandBinding Command="self:KGPcommands.CancelNEW" CanExecute="AlwaysTrueForCanExecute" Executed="Cancel_Executed" />
    </Window.CommandBindings>
    
    <Grid Background="LightGray">
        <TextBox Background="White" Name="DirsNote" Margin="10,10,9.6,55.4" TextWrapping="Wrap">
        </TextBox>
        <Button Margin="54,120,144.6,19.4" Command="self:KGPcommands.ApplyChangesNEW">Zapisz notatke</Button>
        <Button Margin="149,120,59.6,19.4" Command="self:KGPcommands.CancelNEW">Anuluj</Button>
    </Grid>
</Window>
